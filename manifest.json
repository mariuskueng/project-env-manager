{
  "name": "Project ENV Manager",
  "description": "Quick access for Dev/Staging/Production links for multiple projects (with Upsun support)",
  "version": "0.3.0",
  "manifest_version": 3,
  "permissions": ["storage", "tabs", "activeTab", "scripting"],
  "options_ui": {
    "page": "options_ui/options_ui.html",
    "open_in_tab": true
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "host_permissions": ["https://*.upsun.com/*"]
}
